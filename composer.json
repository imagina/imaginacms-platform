{
    "name": "imagina/cms-platform",
    "description": "The AsgardCms application.",
    "keywords": [
        "cms",
        "asgardcms",
        "multilingual",
        "laravel",
        "laravel10"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "anhskohbo/no-captcha": "^3.5",
        "astrotomic/laravel-translatable": "^11.12",
        "barryvdh/laravel-dompdf": "^2.0",
        "cartalyst/sentinel": "^7.0",
        "composer/installers": "~1.0",
        "cviebrock/eloquent-sluggable": "^8.0|^9.0|^10.0",
        "doctrine/dbal": "^3.5",
        "guzzlehttp/guzzle": "^7.2",
        "hisorange/browser-detect": "^4.4.0",
        "imagina/laravel-menus": "^10.0",
        "imagina/laravel-sidebar": "^3.1",
        "imagina/stylist": "^4.0",
        "intervention/image": "^2.5",
        "jantinnerezo/livewire-alert": "^2.2",
        "kalnoy/nestedset": "^6.0",
        "laracasts/presenter": "^0.2.6",
        "laravel/framework": "^10.13",
        "laravel/passport": "^11.5",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "laravelcollective/html": "^6.4",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.11",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^1.7",
        "nwidart/laravel-modules": "^10.0",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.2",
        "ramsey/uuid": "^4.1",
        "sentry/sentry-laravel": "^3.8",
        "spatie/laravel-responsecache": "^7.4",
        "spatie/laravel-sitemap": "^6.3",
        "spatie/robots-txt": "^2.0",
        "stancl/tenancy": "^3.7",
        "symfony/yaml": "^6.2",
        "tightenco/ziggy": "^1.5",
        "typicms/nestablecollection": "^3.0",
        "venturecraft/revisionable": "^1.40",
        "yajra/laravel-datatables-oracle": "^10.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.24",
        "league/flysystem-aws-s3-v3": "^3.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "orchestra/testbench": "^8.5",
        "phpunit/phpunit": "^10",
        "spatie/laravel-ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.27.0",
        "scrutinizer/ocular": "^1.9",
        "spatie/phpunit-snapshot-assertions": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev",
            "dev-imaginacms10.x": "v10.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "ln -s ../Themes/Adminlte/iadmin/ public/iadmin",
            "cp -R ./Themes/Flatly ./Themes/ImaginaTheme"
        ],
        "pre-update-cmd": [],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "php-http/discovery": true,
            "composer/installers": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true

}
