{
    "name": "imagina/cms-platform",
    "description": "The AsgardCms application.",
    "keywords": [
        "cms",
        "asgardcms",
        "multilingual",
        "laravel",
        "laravel8"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "anhskohbo/no-captcha": "^3.5",
        "astrotomic/laravel-translatable": "^11.12",
        "cartalyst/sentinel": "^7.0",
        "cviebrock/eloquent-sluggable": "^10.0",
        "doctrine/dbal": "^3.5",
        "guzzlehttp/guzzle": "^7.2",
        "nwidart/laravel-menus": "^7.0",
        "imagina/stylist": "^3.0",
        "intervention/image": "^2.5",
        "jantinnerezo/livewire-alert": "^2.2",
        "kalnoy/nestedset": "^6.0",
        "laracasts/presenter": "^0.2.4",
        "laravel/framework": "^10.13",
        "laravel/passport": "^11.5",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "laravelcollective/html": "^6.4",
        "livewire/livewire": "^2.11",
        "maatwebsite/laravel-sidebar": "^2.5",
        "mcamara/laravel-localization": "^1.7",
        "nwidart/laravel-modules": "^8.0",
        "pusher/pusher-php-server": "^7.0",
        "ramsey/uuid": "^4.1",
        "symfony/yaml": "^6.2",
        "tightenco/ziggy": "^0.9.4",
        "typicms/nestablecollection": "^3.0",
        "venturecraft/revisionable": "^1.40",
        "yajra/laravel-datatables-oracle": "^10.3",
        "league/flysystem-aws-s3-v3": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.5.10",
        "predis/predis": "^1.1",
        "fakerphp/faker": "^1.9.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev",
            "dev-imaginacms8.x": "v8.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "ln -s ../Themes/Adminlte/iadmin/ public/iadmin",
            "cp -R ./Themes/Flatly ./Themes/ImaginaTheme"
        ],
        "pre-update-cmd": [],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
