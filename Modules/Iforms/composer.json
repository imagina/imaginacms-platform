{
    "name": "imagina/iforms-module",
    "type": "asgard-module",
    "description": "Iforms module for AsgardCMS",
    "keywords": ["forms","emails","Send Emails"],
    "license": "MIT",
    "authors": [
        {
            "name": "Desarrollo Imagina",
            "email": "soporte@imaginacolombia.com"
        }
    ],
    "support": {
        "email": "soporte@imaginacolombia.com",
        "issues": "https://github.com/imagina/asgardcms-iforms/issues",
        "source": "https://github.com/imagina/asgardcms-iforms"
    },
    "require": {
        "php": ">=7.0.0",
        "composer/installers": "~1.0",
        "imagina/core-module": "4.0.x-dev",
        "maatwebsite/excel": "^3.1",
        "anhskohbo/no-captcha": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "3.5.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Iforms\\": ".",
            "Modules\\": "Modules/"
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Iforms\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Iforms\\Providers\\IformsServiceProvider"
            ],
            "aliases": {
            
            }
        },
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
    
}
